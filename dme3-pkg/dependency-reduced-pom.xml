<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.att.aft</groupId>
  <artifactId>dme2</artifactId>
  <name>dme2-3.x</name>
  <version>3.1.200-SNAPSHOT</version>
  <developers>
    <developer>
      <name>Pavani</name>
      <organization>ATT</organization>
      <organizationUrl>www.att.com</organizationUrl>
    </developer>
    <developer>
      <name>Andrew</name>
      <organization>ATT</organization>
      <organizationUrl>www.att.com</organizationUrl>
    </developer>
  </developers>
  <licenses>
    <license>
      <name>BSD License</name>
    </license>
  </licenses>
  <scm />
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.5.1</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.19.1</version>
        <configuration>
          <skipTests>true</skipTests>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <manifestEntries>
                    <Main-Class>com.att.aft.dme2.Version</Main-Class>
                  </manifestEntries>
                </transformer>
              </transformers>
              <relocations>
                <relocation>
                  <pattern>com.att.scld.piccolo</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.piccolo</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.apache</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.apache</shadedPattern>
                  <excludes>
                    <exclude>org.apache.log4j.**</exclude>
                  </excludes>
                </relocation>
                <relocation>
                  <pattern>org.eclipse.jetty</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.jetty</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.mortbay.util</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.ombutil</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.att.scld.grm</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.grm</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.common</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.google.common</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.att.aft.metrics</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.metrics</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.sun.activation</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.activation</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.gson</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.gson</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.codehaus.jackson</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.jackson</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.codehaus.jettison</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.jettison</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.sun.jersey</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.jersey</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.sun.ws.rs</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.sunwsrs</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>javax.ws.rs</pattern>
                  <shadedPattern>com.att.aft.dme2.internal.javaxwsrs</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.apache.tomcat.jdbc</pattern>
                  <shadedPattern>com.att.aft.dme2.jdbc.internal.apache.tomcat.jdbc</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.apache.julic</pattern>
                  <shadedPattern>com.att.aft.dme2.jdbc.internal.apache.juli</shadedPattern>
                </relocation>
              </relocations>
              <artifactSet>
                <excludes>
                  <exclude>javax.servlet:*</exclude>
                  <exclude>com.att.aft.discovery:*</exclude>
                  <exclude>javax.jms:*</exclude>
                  <exclude>com.oracle:*</exclude>
                  <exclude>mysql:*</exclude>
                  <exclude>com.jolbox:*</exclude>
                  <exclude>com.mchange:*</exclude>
                  <exclude>commons-dbcp:*</exclude>
                  <exclude>commons-pool:*</exclude>
                  <exclude>hsqldb:*</exclude>
                  <exclude>postgresql:*</exclude>
                  <exclude>org.slf4j:*</exclude>
                </excludes>
              </artifactSet>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>junit/**</exclude>
                    <exclude>edu/**</exclude>
                    <exclude>javax/servlet/**</exclude>
                    <exclude>javax/servlet/http/**</exclude>
                    <exclude>javax/servlet/resources/**</exclude>
                    <exclude>com/att/aft/discovery/**</exclude>
                    <exclude>jndi.properties</exclude>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                  </excludes>
                </filter>
              </filters>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.10.3</version>
      </plugin>
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <version>2.7</version>
      </plugin>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <version>3.0.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.8.2</version>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>doclint-java8-disable</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-javadoc-plugin</artifactId>
            <version>2.10.4</version>
            <configuration>
              <additionalparam>-Xdoclint:none</additionalparam>
            </configuration>
          </plugin>
          <plugin>
            <artifactId>maven-site-plugin</artifactId>
            <version>3.3</version>
            <executions>
              <execution>
                <id>default-site</id>
                <phase>site</phase>
                <goals>
                  <goal>site</goal>
                </goals>
                <configuration>
                  <reportPlugins>
                    <plugin>
                      <groupId>org.apache.maven.plugins</groupId>
                      <artifactId>maven-javadoc-plugin</artifactId>
                      <configuration>
                        <additionalparam>-Xdoclint:none</additionalparam>
                      </configuration>
                    </plugin>
                  </reportPlugins>
                </configuration>
              </execution>
              <execution>
                <id>default-deploy</id>
                <phase>site-deploy</phase>
                <goals>
                  <goal>deploy</goal>
                </goals>
                <configuration>
                  <reportPlugins>
                    <plugin>
                      <groupId>org.apache.maven.plugins</groupId>
                      <artifactId>maven-javadoc-plugin</artifactId>
                      <configuration>
                        <additionalparam>-Xdoclint:none</additionalparam>
                      </configuration>
                    </plugin>
                  </reportPlugins>
                </configuration>
              </execution>
            </executions>
            <configuration>
              <reportPlugins>
                <plugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-javadoc-plugin</artifactId>
                  <configuration>
                    <additionalparam>-Xdoclint:none</additionalparam>
                  </configuration>
                </plugin>
              </reportPlugins>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>
  <dependencies>
    <dependency>
      <groupId>javax.jms</groupId>
      <artifactId>jms</artifactId>
      <version>1.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>javax.servlet-api</artifactId>
      <version>3.1.0</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>3.6</version>
        <configuration>
          <targetJdk>1.8</targetJdk>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
        <version>3.0.3</version>
        <configuration>
          <effort>Max</effort>
          <threshold>Low</threshold>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
  <distributionManagement>
    <repository>
      <id>ossrh</id>
      <name>swm-repo</name>
      <url>https://oss.sonatype.org/service/local/staging/deploy/maven2/</url>
    </repository>
    <snapshotRepository>
      <id>ossrh</id>
      <url>https://oss.sonatype.org/content/repositories/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>

