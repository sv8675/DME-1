<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://scld.att.com/grm/v1"
	xmlns:tns="http://scld.att.com/grm/types/v1" 	
	xmlns:grm_commontypes="http://scld.att.com/grm/types/v1"
	xmlns:grm_tms="http://scld.att.com/grm/tms/v1"
	xmlns:grm_policy="http://scld.att.com/grm/policy/v1"
	elementFormDefault="qualified" >

	<import schemaLocation="grm_commontypes.xsd" namespace="http://scld.att.com/grm/types/v1" />
	<import schemaLocation="grm_serviceAvailability.xsd" namespace="http://scld.att.com/grm/tms/v1" />
	<import schemaLocation="grm_policy.xsd" namespace="http://scld.att.com/grm/policy/v1" />

	<element name="GRMExceptionDetails">
		<complexType>
			<sequence>
				<element name="code" type="string" />
				<element name="message" type="string" />
			</sequence>
		</complexType>
	</element>

	<!--
		######################################################################
	-->
	<!-- ## Container request/response. -->
	<!--
		######################################################################
	-->
	<element name="addContainerDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerDefinition" type="grm_commontypes:ContainerDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="addContainerDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="updateContainerDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerDefinition" type="grm_commontypes:ContainerDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="updateContainerDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="deleteContainerDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerDefinition" type="grm_commontypes:ContainerDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="deleteContainerDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<!-- name * expression -->
	<element name="findAllContainerDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext" />
			</complexContent>
		</complexType>
	</element>
	<element name="findAllContainerDefinitionResponse">
		<complexType>
			<sequence>
				<element name="containerDefinitionList" type="grm_commontypes:ContainerDefinition" minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<!-- name * expression -->
	<element name="findContainerDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="name" type="string" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerDefinitionResponse">
		<complexType>
			<sequence>
				<element name="containerDefinitionList" type="grm_commontypes:ContainerDefinition"
					minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>

	<!--
		Do you add the associations while adding the container instance??
	-->
	<element name="addContainerVersionDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerDefinition" type="grm_commontypes:ContainerDefinition" />
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
						<element name="serviceVersionDefinitionList" type="grm_commontypes:ServiceVersionDefinition"
							minOccurs="0" maxOccurs="unbounded" />
						<element name="lrmList" type="grm_commontypes:LRM"
							minOccurs="0" maxOccurs="unbounded" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="addContainerVersionDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="updateContainerVersionDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
						<element name="notifListOperationType" type="grm_commontypes:AssociationOperation" />
						<element name="notifListOverrideOperationType" type="grm_commontypes:AssociationOperation" />
						<element name="clearMemoryUsageWeight" type="boolean" minOccurs="0" maxOccurs="1"/>
						<element name="clearCPUUsageWeight" type="boolean" minOccurs="0" maxOccurs="1"/>
						<element name="clearResourceContentionLevel" type="boolean" minOccurs="0" maxOccurs="1"/>
						<element name="clearMemoryUsageWeightOverride" type="boolean" minOccurs="0" maxOccurs="1"/>
						<element name="clearCPUUsageWeightOverride" type="boolean" minOccurs="0" maxOccurs="1"/>
						<element name="clearResourceContentionLevelOverride" type="boolean" minOccurs="0" maxOccurs="1"/>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="updateContainerVersionDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="deleteContainerVersionDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="deleteContainerVersionDefinitionResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="applyContainerVersionDefinitionAssociationRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
						<element name="serviceVersionDefinitionList" type="grm_commontypes:ServiceVersionDefinition"
							minOccurs="0" maxOccurs="unbounded" />
						<element name="lrmList" type="grm_commontypes:LRM"
							minOccurs="0" maxOccurs="unbounded" />
						<element name="associationOperation" type="grm_commontypes:AssociationOperation" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="applyContainerVersionDefinitionAssociationResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>

	<!--
		name should support * expression. If patchVersion is not specified, we
		should get every thing.
	-->
	<!--
		If you dont want to search by version pass -1 in major and -1 in
		minor.
	-->
	<element name="findContainerVersionDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="name" type="string" />
						<element name="version" type="grm_commontypes:VersionDefinition" />
						<element name="routeOffer" type="string" />
						<element name="lrmHostAddress" type="string" />
						<element name="lrmListenPort" type="string" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerVersionDefinitionResponse">
		<complexType>
			<sequence>
				<element name="containerVersionDefinitionList" type="grm_commontypes:ContainerVersionDefinition"
					minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<element name="findContainerVersionDefinitionByCDRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerDefinition" type="grm_commontypes:ContainerDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerVersionDefinitionByCDResponse">
		<complexType>
			<sequence>
				<element name="containerVersionDefinitionList" type="grm_commontypes:ContainerVersionDefinition"
					minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<element name="findAllContainerVersionDefinitionRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findAllContainerVersionDefinitionResponse">
		<complexType>
			<sequence>
				<element name="containerVersionDefinitionList" type="grm_commontypes:ContainerVersionDefinition"
					minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<element name="findContainerVersionDefinitionByRORequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="routeOfferList" type="string" minOccurs="0" maxOccurs="unbounded" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerVersionDefinitionByROResponse">
		<complexType>
			<sequence>
				<element name="containerVersionDefinitionList" type="grm_commontypes:ContainerVersionDefinition"
					minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<!--
		Do you add the associations while adding the container instance??
	-->
	<element name="addContainerInstanceRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
						<element name="containerInstance" type="grm_commontypes:ContainerInstance" />
						<element name="lrm" type="grm_commontypes:LRM" />
						<element name="serviceEndPointList" type="grm_commontypes:ServiceEndPoint"
							minOccurs="0" maxOccurs="unbounded" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="addContainerInstanceResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="updateContainerInstanceRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerInstance" type="grm_commontypes:ContainerInstance" />
						<element name="updateLease" type="boolean" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="updateContainerInstanceResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>
	<element name="deleteContainerInstanceRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerInstance" type="grm_commontypes:ContainerInstance" minOccurs="1" maxOccurs="unbounded"/>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="deleteContainerInstanceResponse">
		<complexType>
			<sequence>
				<element name="result" type="grm_commontypes:Result" />
			</sequence>
		</complexType>
	</element>

	<!-- if both containerName and serviceName are passed in containerName takes precedence and serviceName is ignored. -->
	<element name="findContainerInstanceRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="name" type="string" />
						<element name="version" type="grm_commontypes:VersionDefinition" />
						<element name="lrmHostAddress" type="string" />
						<element name="routeOffer" type="string" />
						<element name="serviceName" type="string" />
						<element name="serviceVersion" type="grm_commontypes:VersionDefinition" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerInstanceResponse">
		<complexType>
			<sequence>
				<element name="containerInstanceList" type="grm_commontypes:ContainerInstance" minOccurs="0" maxOccurs="unbounded" />
			</sequence>
		</complexType>
	</element>
	<!-- *** if lrmList is null, it just retrieves by CVD. -->
	<element name="findContainerInstanceByCVDRequest">
		<complexType>
			<complexContent>
				<extension base="grm_commontypes:RequestContext">
					<sequence>
						<element name="containerVersionDefinition" type="grm_commontypes:ContainerVersionDefinition" />
						<element name="lrmList" type="grm_commontypes:LRM"
							minOccurs="0" maxOccurs="unbounded" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="findContainerInstanceByCVDResponse">
